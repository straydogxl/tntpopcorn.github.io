    function adveflex() {
        var asrc='https://bbs.3dmgame.com/thread-6137460-1-1.html';
        var img='/page/images/adveflex.png';
        var html = '';
        html += '<style>';
        html += '.adveflex{ position: fixed; top: 34px; z-index: 1000; }';
        html += '.adveflex img{  width:200px;}';
        html +='.adveflex .close_btn{position: absolute;right: 0;top: 0;width: 66px;height: 18px;cursor: pointer;z-index: 2;background: url(https://www.3dmgame.com/attach/images/closebt.png) no-repeat;}',
            html += '</style>';
        html += '<div class="adveflex">';
        html += '<div class="close_btn"></div>';
        html += '<a href="'+asrc+'" target="_blank">';
        html += '<img src="'+img+'" alt="">';
        html += '</a>';
        html += '</div>';
        $('body').append(html);
        function js_html() {
            var left = ($(window).width() - 1100) / 2;
            $(".adveflex").css("left", left + 1200)
        }
        js_html()
        $(window).resize(function () {js_html()});
    }
    var timestamp =Date.parse(new Date())/1000;;
    if(timestamp<1615687200){ adveflex()}

    $(".adveflex .close_btn").click(function (e) {
        $(".adveflex").remove();
    });
</script>
</body>
